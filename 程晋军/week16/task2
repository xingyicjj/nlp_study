    <SemanticCache.py>
    bug1:需要处理prompts为空的情况
    def clear_cache(self):
        prompts = self.redis.lrange(self.name + "list", 0, -1)
        if prompts:  # 确保列表不为空
            self.redis.delete(*prompts)
            self.redis.delete(self.name + "list")
            os.unlink(f"{self.name}.index")
            self.index = None
        return 0
